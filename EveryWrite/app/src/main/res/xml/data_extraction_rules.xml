<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Include app data that should be backed up -->
        <include domain="file" path="."/>
        <exclude domain="file" path="databases/notes_database"/>
        <exclude domain="sharedpref" path="."/>
    </cloud-backup>
    <device-transfer>
        <!-- Include app data that should be transferred during device-to-device transfers -->
        <include domain="file" path="."/>
        <exclude domain="file" path="databases/notes_database"/>
        <exclude domain="sharedpref" path="."/>
    </device-transfer>
</data-extraction-rules>